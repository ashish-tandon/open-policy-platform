    <section class="o-section o-section--page-title">
        <div class="c-page-title c-page-title--generic-page  || js-scrollfire scrollfire-fade-up">
    <div class="c-page-title__content">
        <div class="c-page-title__title-container">
                        <h1 class="c-page-title__title" data-character-count="45:70-90,40:91-110,35:111-1000">Bureau de la mairesse et Conseil municipal</h1>
        </div>

        <div class="c-page-title__filter-and-button-share">
                        <button class="c-page-title__button-share || js-open-overlay-share">
                <span class="c-page-title__button-share-text">Partager</span>
