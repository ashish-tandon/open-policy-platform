function ssm_header_searchBox_submit() {
	window.location.href = "/Search.aspx?searchtext=" + encodeURIComponent(document.getElementById("search-box--searchtext").value);
}
document.getElementById("search-box--searchtext").addEventListener("keyup", function(inputEvent) {
	if (inputEvent.key === "Enter") {
		inputEvent.preventDefault();
		ssm_header_searchBox_submit();
	}
});
document.getElementById("search-box--submit").addEventListener("click", ssm_header_searchBox_submit);
