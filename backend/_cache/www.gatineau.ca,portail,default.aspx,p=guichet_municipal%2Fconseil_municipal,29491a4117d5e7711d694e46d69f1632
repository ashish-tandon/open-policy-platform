	function membre(index) {
		var sortie = "";
		
		sortie = sortie + "<div style=\"width: 128px; margin: 0 20px;\">";
		sortie = sortie + "<p align=\"center\">";
		
		if (arrayMembres[index][4].length > 0) {
			sortie = sortie + "<a href=\"" + arrayMembres[index][4] + "\">";
		}
		
		sortie = sortie + "<span style=\"display: block; margin-top: 5px;\"><img src=\"" + arrayMembres[index][5] + "\" border=\"0\" width=\"100%\" alt=\"\" style=\"max-width: 480px;\" /></span>";
		sortie = sortie + "<span style=\"display: block; margin-top: 5px;\"><strong>" + arrayMembres[index][2] + "</strong></span>";
		
		if (arrayMembres[index][4].length > 0) {
			sortie = sortie + "</a>";
		}
		
		if (arrayMembres[index][0].length > 0) {
			sortie = sortie + "<span style=\"display: block; margin-top: 5px;\">" + arrayMembres[index][0] + "</span>";
		}
		
		if (arrayMembres[index][1].length > 0) {
			sortie = sortie + "<span style=\"display: block; margin-top: 5px;\">" + arrayMembres[index][1] + "</span>";
		}
		
		if (arrayMembres[index][3].length > 0) {
			sortie = sortie + "<span style=\"display: block; margin-top: 5px;\">" + arrayMembres[index][3] + "</span>";
		}
		
		sortie = sortie + "</p>";
		sortie = sortie + "</div>";
		
		return sortie;
	}
	
	function membres() {
		var sortie = "";
		
		for (var i = 0; i < arrayMembres.length; i++) {
			if (i == 0 || i == 3) {
				sortie = sortie + "<div style=\"display: flex; flex-wrap: wrap; align-items: baseline; justify-content: center;\">";
			}
			
			sortie = sortie + membre(i);
			
			if (i == 2 || i == arrayMembres.length - 1) {
				sortie = sortie + "</div>";
			}
		}
		
		document.getElementById("DivMembres").innerHTML = sortie;
	}
	
	membres();
</script>
	
	<h1>R&eacute;vision des r&egrave;gles de r&eacute;gie interne</h1>
	<p>Depuis  quelques ann&eacute;es, la Ville de Gatineau a pour objectif de r&eacute;viser ses r&egrave;gles de  r&eacute;gie interne, qui datent de 2001. En ce sens, la Ville s'est engag&eacute;e dans  diverses d&eacute;marches afin d'apporter des modifications &agrave; son r&egrave;glement.  L'Institut du Nouveau Monde propose un <a href="/upload/documents/5adda471-9c02-4fac-9a8d-a7391a0b301d.pdf" target="_blank">rapport</a> qui dresse un portrait comparatif des r&egrave;gles et des  pratiques ainsi que des recommandations quant &agrave; la r&eacute;vision des r&egrave;gles de r&eacute;gie  interne. Nous invitons tous les citoyennes et citoyens qui souhaitent &eacute;mettre  leurs commentaires sur ce rapport &agrave; communiquer avec leur conseiller municipal.  Une rencontre de travail sur le sujet se tiendra en d&eacute;but d'ann&eacute;e 2024 dans  laquelle tous les conseillers municipaux pourront r&eacute;viser ce rapport en vue de  proposer une nouvelle r&eacute;gie interne actualis&eacute;e. </p>
<div style="background-color: #006bb7;" class="boite-de-message">
  <div class="icone aide"></div>
	<div class="texte">
<h1>Qui est votre conseiller municipal?</h1>
<script>
	function MM_ajaxLoader_ConseillerMunicipal() {
		if (document.getElementById("InputTextConseillerMunicipalNumeroImmeuble").value.length < 1) {
			alert("Veuillez entrer un numéro d'immeuble.");
			document.getElementById("InputTextConseillerMunicipalNumeroImmeuble").focus();
		}
		else if (document.getElementById("InputTextConseillerMunicipalNomRue").dataset.rue != "true") {
			alert("Veuillez sélectionner une rue.");
			document.getElementById("InputTextConseillerMunicipalNomRue").focus();
			
			var e = document.createEvent("HTMLEvents");
			e.initEvent("keyup", false, true);
			document.getElementById("InputTextConseillerMunicipalNomRue").dispatchEvent(e);
		}
		else {
			document.getElementById("DivConseillerMunicipalResultat").style.display = "block";
			
			var x = new XMLHttpRequest();
			x.onreadystatechange = function() {
				if (x.readyState == 4 && x.status == 200) {
					var y = JSON.parse(x.responseText);
					
					if (y.length > 0) {
						var z = "<h2>" + y[0].adresse + "</h2>";
						
						z = z + "<p>Le conseiller municipal du " + y[0].adresse + " est&nbsp;:</p>";
						z = z + "<div class=\"colonnes\">";
						z = z + "<div class=\"colonnes-2\">";
						z = z + "<p align=\"center\"><a href=\"/portail/default.aspx?p=guichet_municipal/conseil_municipal/district_" + y[0].district_electoral_numero + "\"><img src=\"/docs/guichet_municipal/conseil_municipal/district_" + y[0].district_electoral_numero + ".jpg\" border=\"0\" width=\"100%\" alt=\"\" style=\"max-width: 150px;\" /></a></p>";
						z = z + "</div>";
						z = z + "<div class=\"colonnes-10\">";
						z = z + "<h3><a href=\"/portail/default.aspx?p=guichet_municipal/conseil_municipal/district_" + y[0].district_electoral_numero + "\">" + y[0].conseiller_municipal + "</a></h3>";
						z = z + "<p>" + y[0].district_electoral + "<br />" + y[0].secteur + "</p>";
						z = z + "<p><button onclick=\"window.location = '/portail/default.aspx?p=guichet_municipal/conseil_municipal/district_" + y[0].district_electoral_numero + "&ref=conseiller-municipal';\">Plus de renseignements</button></p>";
						z = z + "</div>";
						z = z + "</div>";
						
						document.getElementById("DivConseillerMunicipalResultatSortie").innerHTML = z;
					}
					else {
						document.getElementById("DivConseillerMunicipalResultatSortie").innerHTML = "<p>Nous n'avons pas été en mesure de trouver un conseiller municipal à partir de l'adresse entrée.</p>";
					}
					
					gatineauCa.redessinerPage();
					
					commun.defilerVers(document.getElementById("DivConseillerMunicipalResultatSortie"));
				}
			}
			
			x.open("GET", "/json/information_propriete.aspx?langue=fr-CA&numero_immeuble=" + encodeURIComponent(document.getElementById("InputTextConseillerMunicipalNumeroImmeuble").value) + "&nom_rue=" + encodeURIComponent(document.getElementById("InputTextConseillerMunicipalNomRue").value), true);
			x.send(null);
		}
	}
</script>
<p>Entrez votre num&eacute;ro d'immeuble et le nom de la rue sur laquelle vous habitez pour conna&icirc;tre votre conseiller municipal.</p>

