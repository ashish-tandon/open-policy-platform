<h1>Carte du district électoral</h1>
<style>
	#DivCarte .zone {
		width: 100px !important;
		height: unset !important;
		font: bold 11px var(--font-family) !important;
		text-align: center !important;
		color: #fff !important;
		text-shadow: 0 0 5px #000, 0 0 5px #000, 0 0 5px #000, 0 0 5px #000 !important;
	}
</style>
<script src="../turf.min.js" charset="utf-8"></script>
<link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" crossorigin="" />
<script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js" crossorigin=""></script>
<script src="https://unpkg.com/esri-leaflet@3.0.12/dist/esri-leaflet.js"></script>
<script src="https://unpkg.com/esri-leaflet-vector@4.2.3/dist/esri-leaflet-vector.js"></script>
<div style="height: 50vh; z-index: 0;" class="carte-geographique" id="DivCarte"></div>
<script>
	function MM_ajaxLoader_DistrictsElectoraux(numeroDistrictElectoral) {
		var x = new XMLHttpRequest();
		x.onreadystatechange = function() {
			if (x.readyState == 4 && x.status == 200) {
				var y = JSON.parse(x.responseText);
				
				var map = L.map(document.getElementById("DivCarte"));
				
				map.createPane("world_hillshade");
				map.createPane("world_topo");
				map.createPane("canada_hillshade");
				map.createPane("canada_topo");

				const worldHillshade = L.esri.tiledMapLayer({
						url: "https://services.arcgisonline.com/arcgis/rest/services/Elevation/World_Hillshade/MapServer",
						pane: "world_hillshade"
					});

				const canadaHillshade = L.esri.tiledMapLayer({
						url: 'https://tiles.arcgis.com/tiles/B6yKvIZqzuOr0jBR/arcgis/rest/services/Canada_Hillshade/MapServer',
						pane: "canada_hillshade"
					});

				const worldTopoCanadaStyle = L.esri.Vector.vectorTileLayer("6d0ed88458c6429d99331260fb7bf2b0", {
						pane: "world_topo"
					});

				const canadaTopo = L.esri.Vector.vectorTileLayer(
					"https://tiles.arcgis.com/tiles/B6yKvIZqzuOr0jBR/arcgis/rest/services/Canada_Topographic/VectorTileServer", {
						pane: "canada_topo"
					});

				const canadian_topo = L.layerGroup([
						canadaTopo,
						worldTopoCanadaStyle,
						canadaHillshade,
						worldHillshade
					]).addTo(map);
				
				map.createPane("polygons");
				
				var coordinates = new Array();
				
				for (var i = 0; i < y.features[numeroDistrictElectoral - 1].geometry.coordinates[0].length; i++) {
					coordinates[coordinates.length] = new Array(y.features[numeroDistrictElectoral - 1].geometry.coordinates[0][i][1], y.features[numeroDistrictElectoral - 1].geometry.coordinates[0][i][0]);
				}
				
				var polygon = L.polygon(coordinates, {color: '#000', weight: 2, pane: "polygons"}).addTo(map);
				
				var marker = L.marker([turf.pointOnFeature(y.features[numeroDistrictElectoral - 1].geometry).geometry.coordinates[1], turf.pointOnFeature(y.features[numeroDistrictElectoral - 1].geometry).geometry.coordinates[0]], {icon: L.divIcon({html: "District " + y.features[numeroDistrictElectoral - 1].properties.DISTRICT + " (" + y.features[numeroDistrictElectoral - 1].properties.NUM + ")", className: "zone", iconSize: [100, 100], iconAnchor: [50, 0], popupAnchor: [0, 0]})}).addTo(map);
				
				map.fitBounds(polygon.getBounds());
			}
		}
		
		x.open("GET", "../districts_electoraux.json", true);
		x.send(null);
	}

	MM_ajaxLoader_DistrictsElectoraux(2);
</script>

<div id="DivMembreConseilMunicipalFonctionsPolitiques"></div>

<script>
	function MM_ajaxLoader_MembreConseilMunicipalFonctionsPolitiques(membreConseilMunicipal) {
		var x = new XMLHttpRequest();
		x.onreadystatechange = function() {
			if (x.readyState == 4 && x.status == 200) {
				var y = JSON.parse(x.responseText);
				var sortie = "";
				
				for (var i = 0; i < y.length; i++) {
					if (y[i].comite_commission.length > 0 && y[i].membre_conseil_municipal.length > 0 && y[i].membre_conseil_municipal == membreConseilMunicipal && y[i].poste.length > 0 && y[i].afficher_page_membre_conseil_municipal == "1") {
						sortie = sortie + "<li>" + y[i].poste + ", " + (y[i].comite_commission_adresse_url.length > 0 ? "<a href=\"" + y[i].comite_commission_adresse_url + "\">" + y[i].comite_commission + "</a>" : y[i].comite_commission) + "</li>"
					}
				}
				
				if (sortie.length > 0) {
					sortie = "<h1>Fonctions politiques</h1><ul>" + sortie + "</ul>";
				}
				
				document.getElementById("DivMembreConseilMunicipalFonctionsPolitiques").innerHTML = sortie;

				gatineauCa.redessinerPage();
			}
		}
		
		x.open("GET", "/json/membres_conseil_municipal_fonctions_politiques.aspx?langue=fr-CA", true);
		x.send(null);
	}

	MM_ajaxLoader_MembreConseilMunicipalFonctionsPolitiques("Gilles Chagnon");
</script>

<h1>Priorit&eacute;s pour le pr&eacute;sent mandat</h1>
<ul type="disc">
  <li>Faciliter certains processus pour que les       dossiers se r&egrave;glent plus rapidement </li>
  <li>Travailler efficacement en &eacute;quipe pour que le       secteur d'Aylmer voit la construction d'&eacute;cocentre, une biblioth&egrave;que       moderne et des ar&eacute;nas fonctionnels</li>
  <li>Trouver des &eacute;conomies pour contr&ocirc;ler la hausse       des taxes</li>
  <li>Assurer       la s&eacute;curit&eacute; des citoyens de Lucerne et leur garantir des services de       proximit&eacute; de qualit&eacute;
  <li>S'assurer que les infrastructures r&eacute;pondent aux d&eacute;veloppements        urbains pr&eacute;vus du secteur</li>
    <li>&Eacute;laborer une solution moderne pour les besoins en transport        du secteur Aylmer</li>
    <li>Poursuivre le  programme de sensibilisation du respect du code de la route</li>
</ul>
