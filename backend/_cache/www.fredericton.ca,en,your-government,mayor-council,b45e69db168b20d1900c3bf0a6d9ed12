              <div class="field__item content-row" data-visible>  <div class="paragraph paragraph--type--content-row paragraph--view-mode--default">
          
      <div class="field field--name-field-row-component field--type-entity-reference-revisions field--label-hidden field__items content-row__component">
              <div class="field__item">  <div class="paragraph paragraph--type--images paragraph--view-mode--default">
          <div class="images-carousel">
  <div class="images-carousel__slider swiper">
            <div class="field field--name-field-images-image field--type-entity-reference-revisions field--label-hidden field__items images-carousel__slides swiper-wrapper">
                  <div class="field__item images-carousel__slide swiper-slide">  <div class="paragraph paragraph--type--image paragraph--view-mode--default">
          
<figure >
      
            <div class="field field--name-field-image field--type-entity-reference field--label-hidden field__item">  <img loading="lazy" src="/sites/default/files/styles/16_9/public/2023-05/city_council_2021-2025_4x6.jpeg?itok=t-fppvTS" width="1920" height="1080" typeof="foaf:Image" class="image-style-_6-9" />


