<script>
	function MM_ajaxLoader_MembreConseilMunicipalFonctionsPolitiques(membreConseilMunicipal) {
		var x = new XMLHttpRequest();
		x.onreadystatechange = function() {
			if (x.readyState == 4 && x.status == 200) {
				var y = JSON.parse(x.responseText);
				var sortie = "";
				
				for (var i = 0; i < y.length; i++) {
					if (y[i].comite_commission.length > 0 && y[i].membre_conseil_municipal.length > 0 && y[i].membre_conseil_municipal == membreConseilMunicipal && y[i].poste.length > 0 && y[i].afficher_page_membre_conseil_municipal == "1") {
						sortie = sortie + "<li>" + y[i].poste + ", " + (y[i].comite_commission_adresse_url.length > 0 ? "<a href=\"" + y[i].comite_commission_adresse_url + "\">" + y[i].comite_commission + "</a>" : y[i].comite_commission) + "</li>"
					}
				}
				
				if (sortie.length > 0) {
					sortie = "<h1>Fonctions politiques</h1><ul>" + sortie + "</ul>";
				}
				
				document.getElementById("DivMembreConseilMunicipalFonctionsPolitiques").innerHTML = sortie;

				gatineauCa.redessinerPage();
			}
		}
		
		x.open("GET", "/json/membres_conseil_municipal_fonctions_politiques.aspx?langue=fr-CA", true);
		x.send(null);
	}

	MM_ajaxLoader_MembreConseilMunicipalFonctionsPolitiques("Maude Marquis-Bissonnette");
</script>

