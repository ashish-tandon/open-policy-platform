var membName = true;
var membDistrict = true;
var membParty = true;
//var membPhone = true;
//var membEmail = true;


buildTable = function(){
	var a = "<thead><tr><td><a href='javascript: reload(1)'>Name</a></td><td><a href='javascript: reload(2)'>District</a></td><td><a href='javascript: reload(3)'>Party</a></td><td>Phone</td><td>Email</td></tr></thead><tbody>"
	for (var i=0;i<=members.length-1;i++){
		a += "<tr><td>"+members[i].name+"</td><td>"+members[i].district+"</td><td>"+members[i].party+"</td><td>"+members[i].phone+"</td><td>"+members[i].email+"</td></tr>";
	}
	myTable = $("#table");
	myTable.append(a+"</tbody>");
	myTable.addClass("table");
}

buildTable();


function reload(type){
	console.log(membName);
	if(type===1){
		if(membName === true){
			membName = false;
			members = _.orderBy(members, "name", "asc");
		}else{
			membName = true;
			members = _.orderBy(members, "name", "desc");
		}
	}else if(type===2){
		if(membDistrict === true){
			membDistrict = false;
			members = _.orderBy(members, "district", "asc");
		}else{
			membDistrict = true;
			members = _.orderBy(members, "district", "desc");
		}
	}else{
		if(membParty === true){
			membParty = false;
			members = _.orderBy(members, "party", "asc");
		}else{
			membParty = true;
			members = _.orderBy(members, "party", "desc");
		}
	}
	
	$("#table").empty();
	buildTable();
}