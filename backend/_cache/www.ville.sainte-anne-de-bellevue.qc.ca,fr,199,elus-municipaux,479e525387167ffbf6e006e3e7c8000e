var dropdownItems = document.querySelectorAll('.navbar-nav .dropdown-toggle');

    dropdownItems.forEach(function (dropdownItem) {
        dropdownItem.addEventListener('mouseover', function () {
        if ($(window).width() > 767) {
            //console.log('redt--> ');
            var dropdownMenu = this.nextElementSibling;

            if (dropdownMenu) {
                var windowWidth = $(window).width();
                var dd = windowWidth - ($('.menu_pc #first_lv_9').offset().left + $('.menu_pc #first_lv_9').width());
                var dropdownMenuLeft = this.getBoundingClientRect().left;


                var dropdownMenuDistanceFromLeft = windowWidth - dropdownMenuLeft;


                var distance =dropdownMenuDistanceFromLeft-dd;
                var distance = distance-80+40;
                var menuPcElement = document.getElementsByClassName('menu_pc')[0];


                var menuPcWidth = menuPcElement.clientWidth;
                dropdownMenu.style.transform = 'translateX('+distance+'px)';

                dropdownMenu.style.width = menuPcWidth+'px';

            }
        }
        });
    });
