var wbody = '';
var iframeContents = '';
var base_url = 'https://ville.saguenay.ca/';
var last_scroll_position = 0;

$(function () {
            $('#main-menu-mobile').foundation('down', $('#la-ville-et-vie-democratique'));
        $('#main-menu-mobile').foundation('down', $('#conseils-municipaux-et-darrondissement'));
        $('#main-menu-mobile').foundation('down', $('#membres-des-conseils'));
    
    var scroll = 0;

    $(window).scroll(function () {
        scroll = window.pageYOffset;

        if (scroll >= 10) {
            $('.header-menu').css('z-index', 100);
        }
        else {
            $('.header-menu').css('z-index', 1100);
        }
    });
    });

</script>

